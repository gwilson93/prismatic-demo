#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/garretwilson/src/evaluations/endpoint/demo/component/node_modules/.pnpm/@prismatic-io+spectral@9.0.4/node_modules/@prismatic-io/spectral/bin/node_modules:/Users/garretwilson/src/evaluations/endpoint/demo/component/node_modules/.pnpm/@prismatic-io+spectral@9.0.4/node_modules/@prismatic-io/spectral/node_modules:/Users/garretwilson/src/evaluations/endpoint/demo/component/node_modules/.pnpm/@prismatic-io+spectral@9.0.4/node_modules/@prismatic-io/node_modules:/Users/garretwilson/src/evaluations/endpoint/demo/component/node_modules/.pnpm/@prismatic-io+spectral@9.0.4/node_modules:/Users/garretwilson/src/evaluations/endpoint/demo/component/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/garretwilson/src/evaluations/endpoint/demo/component/node_modules/.pnpm/@prismatic-io+spectral@9.0.4/node_modules/@prismatic-io/spectral/bin/node_modules:/Users/garretwilson/src/evaluations/endpoint/demo/component/node_modules/.pnpm/@prismatic-io+spectral@9.0.4/node_modules/@prismatic-io/spectral/node_modules:/Users/garretwilson/src/evaluations/endpoint/demo/component/node_modules/.pnpm/@prismatic-io+spectral@9.0.4/node_modules/@prismatic-io/node_modules:/Users/garretwilson/src/evaluations/endpoint/demo/component/node_modules/.pnpm/@prismatic-io+spectral@9.0.4/node_modules:/Users/garretwilson/src/evaluations/endpoint/demo/component/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/component-manifest.js" "$@"
else
  exec node  "$basedir/../../bin/component-manifest.js" "$@"
fi
